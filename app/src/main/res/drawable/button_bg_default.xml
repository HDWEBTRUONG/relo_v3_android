            android:padding="@dimen/_1dp">
            <solid android:color="@color/white"/>
            <corners android:radius="@dimen/_17dp"/>
            <stroke android:width="@dimen/_2dp" android:color="@color/bg_button_login"/>
        </shape>
    </item>
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle"
            android:padding="@dimen/_1dp">
            <solid android:color="@color/white"/>
            <corners android:radius="@dimen/_17dp"/>
            <stroke android:width="@dimen/_2dp" android:color="@color/bg_button_login_pressed"/>
        </shape>
    </item>
</selector>
